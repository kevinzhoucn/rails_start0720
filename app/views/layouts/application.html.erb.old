<!DOCTYPE html>
<html>
  <head>
    <title>Projects</title>
    <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
    <%= render_stylesheets %>
    <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
    <%= csrf_meta_tags %>
  </head>
  <body class="<%= controller_name %>" data-controller-name="<%= controller_name %>">
    <div class="container">
      <%= render 'common/navigation' %>
    </div>

    <div class="container">
      <%= yield %>
    </div>

  </body>
</html>
